<% content_for :page_actions do %>
  <li><%= button_link_to t(:back_to_polls_list), session[:return_to] || admin_polls_url, :icon => 'icon-arrow-left' %></li>
<% end %>

<%= render :partial => 'spree/shared/error_messages', :locals => { :target => @poll } %>

<%= form_for [:admin, @poll], :method => :put do |f| %>
  <fieldset class="no-border-top">
    <%= render :partial => 'form', :locals => { :f => f } %>

    <%= render :partial => 'spree/admin/shared/edit_resource_links' %>
  </fieldset>
<% end %>
